
public class Rotto {
	public static void main(String[] args) {

		while(true) {
			int [] arr = new int[45];

			for(int i=0; i < arr.length; i++) {
				arr[i]=i+1;
			}
			for(int i=0 ; i<200 ; i++) {
				int x = (int)((Math.random()*45));
				int y = (int)((Math.random()*45));

				int tmp = arr[x];
				arr[x] = arr[y];
				arr[y] = tmp;

			}
			System.out.print("로또 번호 : ");
			for(int i=0; i < 7; i++) {
				System.out.print(arr[i] + " ");
			}
		}
	
}
